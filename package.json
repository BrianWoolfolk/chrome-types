{
  "description": "Types helpers for Chrome extensions",
  "name": "chrome-types-helpers",
  "license": "Apache-2.0",
  "version": "0.0.2",
  "type": "module",
  "exports": {
    "./lib/types": {
      "import": "./lib/types/index.js",
      "require": "./lib/types/index.cjs"
    }
  },
  "scripts": {
    "run:generate": "./bin/generate/index.js",
    "check:generate": "tsc --noEmit npm/index.d.ts && tsc --noEmit npm/platform_app.d.ts",
    "types": "rm lib/types/*.d.ts; tsc lib/types/*.js --declaration --allowJs --emitDeclarationOnly --outDir lib/types/",
    "rollup": "rollup lib/types/index.js -f cjs -o lib/types/index.cjs",
    "prepublishOnly": "npm run rollup && npm run types",
    "tests": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoogleChrome/chrome-types.git"
  },
  "author": "The Chromium Authors",
  "bugs": {
    "url": "https://github.com/GoogleChrome/chrome-types/issues"
  },
  "homepage": "https://github.com/GoogleChrome/chrome-types",
  "devDependencies": {
    "@types/node": "^14.14.22",
    "async-transforms": "^1.0.6",
    "ava": "^3.15.0",
    "colorette": "^1.2.1",
    "fast-glob": "^3.2.4",
    "json5": "^2.1.3",
    "mri": "^1.1.6",
    "node-fetch": "^2.6.1",
    "rollup": "^2.38.5",
    "semver": "^7.3.4",
    "tar-stream": "^2.1.4",
    "tmp": "^0.2.1",
    "typedoc": "^0.20.23",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "html-escaper": "^3.0.0"
  },
  "peerDependencies": {
    "typedoc": "^0.20.23"
  }
}
